{
    "version": 2,
    "name": "upline-frontend",
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/sw.js",
            "headers": {
                "Cache-Control": "no-cache, no-store, must-revalidate",
                "Service-Worker-Allowed": "/"
            },
            "dest": "/sw.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}